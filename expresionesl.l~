%{
#include <stdio.h>
%}
palabra		[a-z A-Z]*|[a-z A-Z 0-9]*

%%
=					{printf( "ASIGNACION: %s\n", yytext ); return (ASIGNACION);}
==					{printf( "COMPARACION: %s\n", yytext ); return (IGUALIGUAL);}
"||"					{printf( "OPERADORES LOGICOS or: %s\n", yytext ); return (OR);}
"!="					{printf( "OPERADORES LOGICOS !=: %s\n", yytext ); return (DIFERENTE);}
"<="					{printf( "OPERADORES LOGICOS <=: %s\n", yytext ); return (MENORIGUAL);}
">="					{printf( "OPERADORES LOGICOS >=: %s\n", yytext ); return (MAYORIGUAL);}
[">"|"<"|"&&"]				{printf( "OPERADORES LOGICOS: %s\n", yytext ); return (OPERADORESLOGICOS);}
[0-9]+					{printf( "NUMERO: %s\n", yytext ); return (NUMERO);}
[a-zA-Z0-9]*				{printf( "IDENTIFICADOR: %s\n", yytext ); return (IDENTIFICADOR);}
\"{palabra}\"				{printf( "PALABRA: %s\n", yytext ); return (PALABRA);}
[0-9]*\.[0-9]+				{printf( "NUMERO REAL: %s\n", yytext ); return (NUMEROREAL);}
["+"|[-]|"/"|"*"|"%"]			{printf( "OPERADORES ARITMETICO: %s\n", yytext ); return (OPERADORESARIT);}
.					{printf( "ERROR: %s\n", yytext ); ;} 
%%
main()
{	
yylex();
}
